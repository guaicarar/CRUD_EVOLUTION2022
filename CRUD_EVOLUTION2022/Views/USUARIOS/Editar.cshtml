@model CRUD_EVOLUTION2022.Models.ViewModel.NuevoUsuariosWiewModels
@{
    ViewBag.Title = "Editar";
}   

<h2>Editar</h2>
@using (Html.BeginForm("Editar", "USUARIOS", FormMethod.Post))

{
    <div class=" row">
        <div class="col-md-12">
            @Html.HiddenFor(d => d.UsuId)
            <div>
                @Html.LabelFor(d => d.UsuNombre)
                @Html.TextBoxFor(d => d.UsuNombre, "", new { @class = "form-control" })
            </div>
            <div>
                @Html.LabelFor(d => d.UsuPASS)
                @Html.TextBoxFor(d => d.UsuPASS, "", new { @class = "form-control" })
            </div>
            <div>

            </div>
            
            

            <div claas="row" style="text-align:right;padding:5px;">
                <input value="Guardar" class="btn btn-primary" type="submit" />
            </div>
        </div>
    </div>
}

